<ioc xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:noNamespaceSchemaLocation="nutz-ioc-0.1.xsd">
	
	<!-- windows 日志采集器监听服务 -->
	<obj name="$setup_AuthListener"	type="com.zxin.mvc.auth.listener.AuthListener">
		<field name="name"><str>auth</str></field>	
	</obj>
		
</ioc>